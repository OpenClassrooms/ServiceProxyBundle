{
  "name": "openclassrooms/service-proxy-bundle",
  "description": "Service Proxy Symfony2 Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\ServiceProxyBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\ServiceProxyBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "openclassrooms/service-proxy" : "2.0",
    "openclassrooms/doctrine-cache-extension": "1.0.*@dev",
    "openclassrooms/doctrine-cache-extension-bundle": "2.0.*@dev",
    "symfony/dependency-injection": "~3.4 || ~4.0",
    "symfony/config": "~3.4 || ~4.0",
    "symfony/http-kernel": "~3.4 || ~4.0",
    "symfony/event-dispatcher": "~3.4 || ~4.0",
    "symfony/yaml": "~3.4 || ~4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.15",
    "phpunit/phpunit": "~7.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}

{
  "name": "openclassrooms/service-proxy-bundle",
  "description": "Service Proxy Symfony2 Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\ServiceProxyBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\ServiceProxyBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "doctrine/orm": "~2.5",
    "openclassrooms/service-proxy" : "1.0.*@dev",
    "openclassrooms/doctrine-cache-extension": "1.0.*@dev",
    "openclassrooms/doctrine-cache-extension-bundle": "1.0.*@dev",
    "symfony/dependency-injection": ">=2.7 <4",
    "symfony/config": ">=2.7 <4",
    "symfony/http-kernel": ">=2.7 <4",
    "symfony/security": ">=2.7 <4",
    "symfony/event-dispatcher": ">=2.7 <4"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "1.*",
    "mockery/mockery": "0.*",
    "phpunit/phpunit": "~5.0",
    "satooshi/php-coveralls": "1.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
